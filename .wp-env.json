{
  "core": "WordPress/WordPress#6.4",
  "phpVersion": "8.1",
  "plugins": [
    "./github-auto-deploy"
  ],
  "themes": [
    "https://downloads.wordpress.org/theme/twentytwentyfour.1.0.zip"
  ],
  "mappings": {
    "wp-content/plugins/github-auto-deploy": "./github-auto-deploy"
  },
  "port": 8888,
  "testsPort": 8889,
  "config": {
    "WP_DEBUG": true,
    "WP_DEBUG_LOG": true,
    "WP_DEBUG_DISPLAY": false,
    "SCRIPT_DEBUG": true,
    "WP_ENVIRONMENT_TYPE": "local",
    "DISABLE_WP_CRON": false
  },
  "env": {
    "development": {
      "port": 8888
    },
    "tests": {
      "port": 8889,
      "config": {
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": true
      }
    }
  }
}
